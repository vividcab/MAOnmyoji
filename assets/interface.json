{
	"name": "MAOnmyoji",
	"url": "https://github.com/vividcab/MAOnmyoji",
	"mirrorchyan_rid": "",
	"mirrorchyan_multiplatform": false,
	"mfa_max_version": "v1.6.5",
	"controller": [{"name": "ADB 默认方式", "type": "Adb"}],
	"resource": [{"name": "官服", "path": ["{PROJECT_DIR}/resource/base"]}],
	"agent": {"child_exec": "python", "child_args": ["-u", "{PROJECT_DIR}/../agent/main.py"]},
	"task": [
		{
			"name": "多角色-每日签到领商店",
			"entry": "ENTRY-多角色每日签到领商店",
			"advanced": ["每日签到领商店任务-指定角色名列表"]
		},
		{
			"name": "启动游戏",
			"entry": "TASK-启动游戏到登录界面"
		},
		{
			"name": "截图测试",
			"entry": "Screenshot",
			"option": ["是否输出 focus 信息"],
			"advanced": ["focus 输出内容"],
			"doc": "图看不清可以拉伸下这个小窗口\n![bydjl](https://mojing.org/tjb)"
		}
	],
	"advanced": {
		"每日签到领商店任务-指定角色名列表": {
			"field": "每日签到领商店任务-指定角色名列表",
			"type": "string",
			"default": "ALL",
			"pipeline_override": {
				"ENTRY-多角色每日签到领商店": {
					"custom_action_param": {
						"rolenames": "{每日签到领商店任务-指定角色名列表}"
					}
				}
			}
		},
		"focus 输出内容": {
			"field": ["用户输入"],
			"type": ["string"],
			"default": ["用户在此处输入信息"],
			"pipeline_override": {"Screenshot": {"focus": {"start": "{用户输入}"}}},
			"doc": "用户在此处输入的内容将在任务节点执行时打印输出"
		}
	},
	"option": {
		"是否输出 focus 信息": {
			"cases": [
				{
					"name": "是",
					"pipeline_override": {"Screenshot": {"focus": {"start": "执行测试截图任务"}}}
				},
				{
					"name": "否",
					"pipeline_override": {"Screenshot": {"focus": {}}}
				}
			],
			"default_case": "否",
			"doc": "若选择否，则不在右侧日志栏打印输出信息"
		}
	}
}
